for (S1, S2, S3 : REAL_SCALARS)
{
    template class RelaxationBlock<SparseMatrix<S1>, Vector<S2>, S3>;
    template class RelaxationBlockJacobi<SparseMatrix<S1>, Vector<S2>, S3>;
    template class RelaxationBlockSOR<SparseMatrix<S1>, Vector<S2>, S3>;
    template class RelaxationBlockSSOR<SparseMatrix<S1>, Vector<S2>, S3>;
}


for (S2 : REAL_SCALARS)
{
#ifdef DEAL_II_WITH_TRILINOS
    template class RelaxationBlock<TrilinosWrappers::SparseMatrix, TrilinosWrappers::MPI::Vector, S2>;
    template class RelaxationBlockJacobi<TrilinosWrappers::SparseMatrix, TrilinosWrappers::MPI::Vector, S2>;
    template class RelaxationBlockSOR<TrilinosWrappers::SparseMatrix, TrilinosWrappers::MPI::Vector, S2>;
    template class RelaxationBlockSSOR<TrilinosWrappers::SparseMatrix, TrilinosWrappers::MPI::Vector, S2>;
#endif
}

